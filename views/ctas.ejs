<h1>CTA template</h1>

<section id="gigs" class="container">
    <h1>Current Action Alerts!</h1>
    <% calls.forEach(listOfCalls=>{ %>
    <div class="gig">
      <h3><%= listOfCalls.title %> </h3>
      <p><%= listOfCalls.description %></p>
      <ul>
        <li>Contact Email:<%= listOfCalls.conact_email %></li>
        <li>Contact Phone:<%= listOfCalls.contact_phone %></li>
        <li><a href="#" class="btn btn-reverse">Apply Now</a></li>
      </ul>
      <div class="tech">
        <small></span></small>
      </div>
    </div>
    <% }) %> 
  </section>
 
</body>

</html>

